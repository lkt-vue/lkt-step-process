{
  "compilerOptions": {
    "outDir": "./dist/",
    "noImplicitAny": true,
    "strictNullChecks": true,
    "noImplicitThis": true,
    "allowSyntheticDefaultImports": true,
    "module": "es6",
    "target": "es6",
    "jsx": "preserve",
    "allowJs": true,
    "composite": false,
    "declaration": true,
    "moduleResolution": "node"
  }
}